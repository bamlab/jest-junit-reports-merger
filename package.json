{
  "name": "@bam.tech/jest-junit-reports-merger",
  "version": "1.0.0",
  "description": "Merge multiple JUnit reports into one",
  "homepage": "https://github.com/bamlab/jest-junit-reports-merger#readme",
  "bugs": {
    "url": "https://github.com/bamlab/jest-junit-reports-merger/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bamlab/jest-junit-reports-merger.git"
  },
  "license": "MIT",
  "author": "TychoTa",
  "main": "dist/index.js",
  "module": "dist/jest-junit-reports-merger.esm.js",
  "typings": "dist/index.d.ts",
  "bin": {
    "jjrm": "bin/jjrm.js"
  },
  "files": [
    "dist",
    "src",
    "bin"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "commander": "^9.4.1",
    "fast-glob": "^3.2.12",
    "fast-xml-parser": "^4.0.12",
    "fs-extra": "^10.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@size-limit/preset-small-lib": "^8.1.0",
    "@tsconfig/recommended": "^1.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.1.5",
    "@types/node": "^18.11.9",
    "dts-cli": "^1.6.0",
    "husky": "^8.0.2",
    "np": "^7.6.2",
    "size-limit": "^8.1.0",
    "ts-jest": "^27.1.4",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/plugin-syntax-flow",
        "jest"
      ]
    }
  },
  "poeple": {
    "name": "Tycho Tatitscheff",
    "email": "https://twitter.com/tychota"
  },
  "size-limit": [
    {
      "path": "dist/jest-junit-reports-merger.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/jest-junit-reports-merger.esm.js",
      "limit": "10 KB"
    }
  ]
}
